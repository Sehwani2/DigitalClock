#ifndef INC_ALARM_H_
#define INC_ALARM_H_

#include "main.h"

typedef struct _AlarmTime{
	uint8_t hour;
	uint8_t minute;
	uint8_t isEnabled;
}AlarmTime;

typedef struct _Alarm{
	uint8_t alarmIndex;
	AlarmTime alarmTime;
}Alarm;

extern Alarm alarm;
#endif /* INC_ALARM_H_ */
