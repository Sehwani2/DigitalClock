#ifndef INC_WATCH_H_
#define INC_WATCH_H_

#include "main.h"

typedef struct _WatchTime{
	uint8_t hours;
	uint8_t minutes;
	uint8_t second;
	uint16_t millisecond;
}WatchTime;

typedef struct _Watch{
	WatchTime Time;
}Watch;



#endif /* INC_WATCH_H_ */
